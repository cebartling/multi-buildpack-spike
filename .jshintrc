    "eqeqeq" : true,
    "immed" : false,
    "noempty" : true,
    "newcap" : true,
    "nonbsp" : true,
    "undef" : true,
    "unused" : true,
    "trailing" : true,

    // errors
    "maxerr" : 1000,

